<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://www.exchange.nl/exchange/wsdata/1.0" version="1.0" xmlns:tns="http://www.exchange.nl/exchange/wsdata/1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <!--RELATIES-->
  <xs:complexType name="Volmacht">
    <xs:sequence>
      <xs:element name="relatienummer" type="xs:int">
        <xs:annotation>
          <xs:documentation>Unieke identificatie</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="crwamCode" type="xs:int">
        <xs:annotation>
          <xs:documentation>CRWAM code van de volmacht</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="naam" type="xs:string">
        <xs:annotation>
          <xs:documentation>Naam van de volmacht; maximaal 100 tekens.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ATP">
    <xs:sequence>
      <xs:element name="relatienummer" type="xs:int">
        <xs:annotation>
          <xs:documentation>Unieke identificatie</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="afmNummer" type="xs:int">
        <xs:annotation>
          <xs:documentation>AFM nummer van de tussenpersoon</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="naam" type="xs:string">
        <xs:annotation>
          <xs:documentation>Naam van de ATP; maximaal 100 tekens.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Verzekeraar">
    <xs:sequence>
      <xs:element name="relatienummer" type="xs:int">
        <xs:annotation>
          <xs:documentation>Unieke identificatie</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="serienummer" type="xs:int">
        <xs:annotation>
          <xs:documentation>Serienummer van de verzekeraar</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="naam" type="xs:string">
        <xs:annotation>
          <xs:documentation>Naam van de verzekeraar; maximaal 50 tekens.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="afdelingsnaam" type="xs:string">
        <xs:annotation>
          <xs:documentation>Naam van de afdeling; maximaal 50 tekens.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Expertisebureau">
    <xs:sequence>
      <xs:element name="relatienummer" type="xs:int">
        <xs:annotation>
          <xs:documentation>Unieke identificatie</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="naam" type="xs:string">
        <xs:annotation>
          <xs:documentation>Naam van dit expertisebureau; maximaal 50
						tekens.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Schadehersteller">
    <xs:sequence>
      <xs:element name="relatienummer" type="xs:int">
        <xs:annotation>
          <xs:documentation>Unieke identificatie</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="naam" type="xs:string">
        <xs:annotation>
          <xs:documentation>Naam van het schadeherstelbedrijf; maximaal
						100 tekens.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="bezoekadres" type="tns:Adres">
        <xs:annotation>
          <xs:documentation>Bezoekersadres van het
						herstelbedrijf</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="postadres" type="tns:Adres">
        <xs:annotation>
          <xs:documentation>Postadres van het schadeherstelbedrijf</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="telefoon" type="xs:string">
        <xs:annotation>
          <xs:documentation>Telefoonnummer van het
						schadeherstelbedrijf; maximaal 15 tekens.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="fax" type="xs:string">
        <xs:annotation>
          <xs:documentation>Faxnummer van het schadeherstelbedrijf;
						maximaal 15 tekens.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="email" type="xs:string">
        <xs:annotation>
          <xs:documentation>Emailadres van het
						schadeherstelbedrijf; maximaal 100 tekens.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="rating" type="xs:string">
        <xs:annotation>
          <xs:documentation>eXchange Rating van het
						schadeherstelbedrijf. Varierend van A t/m E</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="latitude" type="xs:double">
        <xs:annotation>
          <xs:documentation>Breedtegraad van de locatie van het
						schadeherstelbedrijf</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="longitude" type="xs:double">
        <xs:annotation>
          <xs:documentation>Lengtegraad van de locatie van het
						schadeherstelbedrijf</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Claimsmanager">
    <xs:sequence>
      <xs:element name="relatienummer" type="xs:int">
        <xs:annotation>
          <xs:documentation>Unieke identificatie</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="naam" type="xs:string">
        <xs:annotation>
          <xs:documentation>Bedrijfsnaam van deze claimsmanager;
						maximaal 50 tekens.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <!--DOSSIER-->
  <xs:complexType name="Dossier">
    <xs:sequence>
      <!--Metadata-->
      <xs:element name="dossiernummer" type="xs:long">
        <xs:annotation>
          <xs:documentation>Het dossiernummer dat eXchange toegekend heeft
						aan het dossier. Exact 11 cijfers.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="created" type="xs:string">
        <xs:annotation>
          <xs:documentation>Datum waarop het dossier is aangemaakt</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="lastModified" type="xs:string">
        <xs:annotation>
          <xs:documentation>Datum waarop het dossier voor het laatst is
						gewijzigd</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="fotosLastModified" type="xs:string">
        <xs:annotation>
          <xs:documentation>Datum waarop voor het laatst de verzameling
						bijbehorende foto's is aangepast</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="status" type="tns:DossierStatus">
        <xs:annotation>
          <xs:documentation>Status van het dossier in eXchange</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="voortgang" type="tns:DossierVoortgang">
        <xs:annotation>
          <xs:documentation>Voortgangsindicators voor dit
						dossier</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="melder" type="xs:string">
        <xs:annotation>
          <xs:documentation>Officiële naam van degene die dit
						dossier heeft aangemeld; maximaal 100 tekens.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <!--Schadegegevens-->
      <xs:element name="schadedatum" type="xs:date">
        <xs:annotation>
          <xs:documentation>De datum waarop de schade is ontstaan</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="schadeType" type="tns:SchadeType">
        <xs:annotation>
          <xs:documentation>Type van de schade, vooralsnog alleen
						carrosserie schade. Later wordt dit uitgebreid met
						glasschades.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="schadeOorzaak" type="tns:SchadeOorzaak">
        <xs:annotation>
          <xs:documentation>Oorzaak van de carrosserie schade</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="waarborgfonds" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>Melding waarborgfonds.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="voertuig" type="tns:Voertuig">
        <xs:annotation>
          <xs:documentation>Voertuig-informatie</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="schade" type="tns:CarrosserieSchade">
        <xs:annotation>
          <xs:documentation>Schade-informatie</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="herstelGereed" type="xs:date">
        <xs:annotation>
          <xs:documentation>Vermoedde datum waarop het herstel
						gereed is.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="voorexpertiseAanvraag" type="tns:VoorexpertiseAanvraag">
        <xs:annotation>
          <xs:documentation>Gevuld indien de schadehersteller
						voorexpertise heeft aangevraagd</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="voorexpertiseResultaat" type="tns:ExpertiseResultaat">
        <xs:annotation>
          <xs:documentation>Gevuld indien het expertisebureau
						voorexpertise heeft uitgevoerd</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="calculaties">
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="calculatie" nillable="true" type="tns:Calculatie">
              <xs:annotation>
                <xs:documentation>Calculaties die bij dit dossier
									zijn geüpload</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <!--Verzekeringsgegevens-->
      <xs:element minOccurs="0" name="polisdata" type="tns:Polisdata">
        <xs:annotation>
          <xs:documentation>Polisinformatie over het dossier</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="verzekerde" type="tns:Persoon">
        <xs:annotation>
          <xs:documentation>Informatie over de verzekerde op dit
						dossier</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="berijder" type="tns:Persoon">
        <xs:annotation>
          <xs:documentation>Informatie over de berijder op dit
						dossier, indien deze afwijkt van de verzekerde</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="dekking" type="tns:Dekking">
        <xs:annotation>
          <xs:documentation>Controle of er dekking is afgegeven op
						dit dossier. Ja/Nee of Onbekend</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="opmerkingDekking" type="xs:string">
        <xs:annotation>
          <xs:documentation>Toelichting op de afgegeven dekking;
						maximaal 250 tekens.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <!--Betrokken partijen-->
      <xs:element minOccurs="0" name="referenties" type="tns:Referenties">
        <xs:annotation>
          <xs:documentation>Mogelijkheid tot het registeren van
						eigen unieke identifiers op het dossier per rol.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="verzekeraar" type="tns:Verzekeraar">
        <xs:annotation>
          <xs:documentation>De verzekeraar die de groene kaart van dit
						dossier heeft afgegeven.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="schadehersteller" type="tns:Schadehersteller">
        <xs:annotation>
          <xs:documentation>De schadehersteller die dit dossier afhandelt.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="dealer" type="tns:Schadehersteller">
        <xs:annotation>
          <xs:documentation>De dealer die bij dit dossier betrokken is.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="expertisebureau" type="tns:Expertisebureau">
        <xs:annotation>
          <xs:documentation>Het expertisebureau dat de eventuele expertise
						op dit dossier uitvoert.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="volmacht" type="tns:Volmacht">
        <xs:annotation>
          <xs:documentation>Indien aanwezig, de volmacht die het
						dossier administratief afhandelt.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="atp" type="tns:ATP">
        <xs:annotation>
          <xs:documentation>Indien aanwezig, de tussenpersoon die de
						schade heeft gemeld.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="claimsmanager" type="tns:Claimsmanager">
        <xs:annotation>
          <xs:documentation>Indien aanwezig, de claimsmanager die
						namens de verzekeraar/volmacht het dossier
						administratief afhandelt.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <!--COMPLEXE ELEMENTEN-->
  <xs:complexType name="Foto">
    <xs:sequence>
      <xs:element name="created" type="xs:string">
        <xs:annotation>
          <xs:documentation>Datum waarop de foto initieel is aangeboden
						aan eXchange</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="dossiernummer" type="xs:long">
        <xs:annotation>
          <xs:documentation>Dossiernummer waarop deze foto
						toebehoort</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="fotoBytes" type="xs:base64Binary">
        <xs:annotation>
          <xs:documentation>Aantal bytes van de foto; foto's mogen
						maximaal 200Kb groot zijn.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="index" type="xs:int">
        <xs:annotation>
          <xs:documentation>Plaats van de foto in de thumbnail lijst;
						een waarde tussen 0 en 9</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="lastModified" type="xs:string">
        <xs:annotation>
          <xs:documentation>Datum wanneer de foto voor het laatst is
						aangepast</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="mimeType" type="xs:string">
        <xs:annotation>
          <xs:documentation>Mime type van de foto; maximaal 50 tekens</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DossierCreatieResultaat">
    <xs:sequence>
      <xs:element minOccurs="0" name="dossiernummer" type="xs:long"/>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="messages" type="xs:string"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Polisdata">
    <xs:sequence>
      <xs:element name="polisnummer" type="xs:string">
        <xs:annotation>
          <xs:documentation>Polisnummer van de polis; maximaal 20
						tekens.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="eigenRisicoCarrosserie" type="xs:double">
        <xs:annotation>
          <xs:documentation>Eigen risico; mag niet kleiner zijn de 0</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="product" type="xs:string">
        <xs:annotation>
          <xs:documentation>Naam van het verzekeringsprocuct; maximaal
						250 tekens.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="soortDekking" type="tns:SoortDekking">
        <xs:annotation>
          <xs:documentation>Soort dekking.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="ingangsdatum" type="xs:date">
        <xs:annotation>
          <xs:documentation>Datum waarop de polis ingaat</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="einddatum" type="xs:date">
        <xs:annotation>
          <xs:documentation>Datum waarop de polis in verstreken</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="btw" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>Inclusief BTW (true)
						Exclusief BTW (false)</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Persoon">
    <xs:sequence>
      <xs:element minOccurs="0" name="voorletters" type="xs:string">
        <xs:annotation>
          <xs:documentation>Voorletters van de persoon; maximaal
						50 tekens.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="tussenvoegsel" type="xs:string">
        <xs:annotation>
          <xs:documentation>Tussenvoegsel van de persoon;
						maximaal 20 tekens.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="achternaam" type="xs:string">
        <xs:annotation>
          <xs:documentation>Achternaam van de persoon; maximaal 50
						tekens.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="geboortedatum" type="xs:date">
        <xs:annotation>
          <xs:documentation>Geboortedatum van de persoon</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="adres" type="tns:Adres">
        <xs:annotation>
          <xs:documentation>Het adres</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="email" type="xs:string">
        <xs:annotation>
          <xs:documentation>Email adres van de persoon; maximaal 50
						tekens.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="mobiel" type="xs:string">
        <xs:annotation>
          <xs:documentation>Mobiel telefoonnummer van de persoon;
						maximaal 15 tekens.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="telefoon" type="xs:string">
        <xs:annotation>
          <xs:documentation>Telefoonnummer van de persoon;
						maximaal 15 tekens.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="bankrekening" type="xs:string">
        <xs:annotation>
          <xs:documentation>Bankrekening nummer van de persoon;
						maximaal 18 tekens.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Referenties">
    <xs:sequence>
      <xs:element minOccurs="0" name="expertise" type="xs:string">
        <xs:annotation>
          <xs:documentation>Referentienummer dat het
						expertisebureau hanteert; maximaal 20 tekens.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="schadehersteller" type="xs:string">
        <xs:annotation>
          <xs:documentation>Referentienummer dat het
						schadeherstelbedrijf hanteert; maximaal 20 tekens.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="verzekeraar" type="xs:string">
        <xs:annotation>
          <xs:documentation>Referentienummer dat de
						verzekeringsmaatschappij hanteert; maximaal 20 tekens.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="volmacht" type="xs:string">
        <xs:annotation>
          <xs:documentation>Referentienummer dat de volmacht
						hanteert; maximaal 20 tekens.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="atp" type="xs:string">
        <xs:annotation>
          <xs:documentation>Referentienummer dat de
						tussenpersoon hanteert; maximaal 20 tekens.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="claimsmanager" type="xs:string">
        <xs:annotation>
          <xs:documentation>Referentienummer dat de
						claimsmanager hanteert; maximaal 20 tekens.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Voertuig">
    <xs:sequence>
      <xs:element name="kenteken" type="xs:string">
        <xs:annotation>
          <xs:documentation>Voor Nederlandse kentekens wordt het kenteken
						verwacht zonder streepjes;
						dus 29DRFF en niet 29-DR-FF. Maximaal 15 tekens.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element default="true" name="nederlandsKenteken" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>Aangeven of het voertuig een nederlands
						kenteken voert</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="merk" type="xs:string">
        <xs:annotation>
          <xs:documentation>Merk van het voertuig; maximaal 50 tekens.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="model" type="xs:string">
        <xs:annotation>
          <xs:documentation>Model van het voertuig; maximaal 50 tekens.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="type" type="xs:string">
        <xs:annotation>
          <xs:documentation>Type van het voertuig; maximaal 50 tekens</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="voertuigsoort" type="tns:VoertuigType">
        <xs:annotation>
          <xs:documentation>Type voertuigsoort, bron RDW</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="meldcode" type="xs:int">
        <xs:annotation>
          <xs:documentation>Meldcode van het voertuig; vier-cijfering
						positief getal</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="vinnummer" type="xs:string">
        <xs:annotation>
          <xs:documentation>Vinnummer van het voertuig; exact 17
						tekens</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="2" minOccurs="0" name="brandstof" type="tns:BrandstofType">
        <xs:annotation>
          <xs:documentation>Brandstof van het voertuig</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="kleur" type="xs:string">
        <xs:annotation>
          <xs:documentation>Kleur van het voertuig; maximaal 30 tekens.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="carrosserieVariant" type="tns:CarrosserieType">
        <xs:annotation>
          <xs:documentation>Carrosserie variant van het voertuig</xs:documentation>
        </xs:annotation>
      </xs:element>
