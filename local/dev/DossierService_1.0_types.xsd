        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="aantalZitplaatsen" type="xs:int">
        <xs:annotation>
          <xs:documentation>Aantal zitplaatsen van het voertuig; waarde
						ligt tussen 1 en 9</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="bouwjaar" type="xs:int">
